{
  "manifest_version": 3,
  "name": "Trans Doctolib",
  "version": "0.3.1",
  "description": "Trans Doctolib est une extension navigateur qui permet de voir sur le profil Doctolib d'un praticien, s'il a sign√© une tribune pro ou anti-trans.",
  "icons": {
    "48": "icons/logo.png"
  },
  "developer": {
    "name": "Millefeuille",
    "url": "https://github.com/Millefeuille42"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.doctolib.fr/*",
    "https://doctolib.fr/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.doctolib.fr/*",
        "https://doctolib.fr/*"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "static/doctolib.json"
      ],
      "matches": [
        "https://www.doctolib.fr/*",
        "https://doctolib.fr/*"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "de0d568e3f7b1151b7ae4c685cd85ec2356bf622@millefeuille"
    },
    "gecko_android": {
      "id": "de0d568e3f7b1151b7ae4c685cd85ec2356bf622@millefeuille"
    }
  }
}
